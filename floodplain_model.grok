  K to tecplot
  conductivity.dat

  Read elemental porosity from file
  anEmptyFile

  Porosity to tecplot
  porosity.dat

! definition of face sets and node sets
  clear chosen nodes
    choose nodes top gb
      ./AdditionalFiles/bc_ditches_and_arbach.nchos
    create node set
      bc_ditches_and_arbach
  clear chosen nodes

  clear chosen faces
    choose faces top gb
      ./AdditionalFiles/rr_crop.echos
    create face set from chosen faces
      rr_cropland
  clear chosen faces

  clear chosen faces
    choose faces top gb
      ./AdditionalFiles/rr_fp.echos
    create face set from chosen faces
      rr_floodplain
  clear chosen faces
  
  clear chosen faces
    choose faces top gb
      ./AdditionalFiles/rr_ss.echos
    create face set from chosen faces
      rr_sandstone
  clear chosen faces
  
  clear chosen faces
    choose faces top gb
      ./AdditionalFiles/rr_urban.echos
    create face set from chosen faces
      rr_urban
  clear chosen faces

  clear chosen nodes
    choose nodes list
      ./AdditionalFiles/bcAmmerIn.nlist
    create node set
      bc_ammer_in
  clear chosen nodes

  clear chosen nodes
    choose nodes list
      ./AdditionalFiles/bcAmmerOut.nlist
    create node set
      bc_ammer_out
  clear chosen nodes

  clear chosen nodes
    choose nodes list
      ./AdditionalFiles/bc_neckar.nlist
    create node set
      bc_neckar
  clear chosen nodes

  clear chosen nodes
    choose nodes top gb
      ./AdditionalFiles/bc_river.nchos
    create node set
      river_set
  clear chosen nodes

!initial conditions
  use domain type
  porous media

  choose nodes all
    ! Initial head surface elevation
    Restart file for heads
    initial.hen
  clear chosen nodes

!boundary conditions
  boundary condition  
    type
      simple drain

    name
      bc_ditch_drain
      
    node set
      bc_ditches_and_arbach

    time value table
      0.0 0.001 1.e-1 
    end
      
    tecplot output
  end

  boundary condition
    type
      flux

    name
      bc_rain_cropland

    face set
      rr_cropland

    time value table
      0.0 1.777660e-09
    end
  end

  boundary condition
    type
      flux

    name
      bc_rain_floodplain

    face set
      rr_floodplain

    time value table
      0.0 1.187748e-09
    end
  end

  boundary condition
    type
      flux

    name
      bc_rain_sandstone

    face set
      rr_sandstone

    time value table
      0.0 5.159632e-10
    end
  end

  boundary condition
    type
      flux

    name
      bc_rain_urban

    face set
      rr_urban

    time value table
      0.0 1.489827e-09
    end
  end

  boundary condition
    type
      head

    name
      bc_ammer_in

    node set
      bc_ammer_in
    
    time value table
      0.0   3.463483e+02
    end

    tecplot output
  end

  boundary condition
    type
      head

    name
      bc_ammer_out

    node set
      bc_ammer_out

    time value table
      0.0 3.381526e+02
    end
    
    tecplot output
  end

  boundary condition
    type
      head

    name
      bc_neckar

    node set
      bc_neckar

    time file table
      0.0   ./AdditionalFiles/bc_neckar.lst
    end
    
    tecplot output
  end

  boundary condition
    type
      simple river

    name
      bc_river

    node set
      river_set

    time file table
      0.0   ./AdditionalFiles/bc_river.lst
    end
    
    tecplot output
  end

!simulation and time step control parameters
  head control
    2.50
  saturation control
    0.5
  newton iteration control
    12
  newton maximum iterations
    20
  maximum timestep multiplier
    1.25
  minimum timestep multiplier
    0.05

  flow solver maximum iterations
    1000
  compute underrelaxation factor
  jacobian epsilon
    5.0d-6
  newton absolute convergence criteria
    1.0d-5
  newton residual convergence criteria
    1.0d-5

!output
  Time output scientific format
  output times
    3e12
  end

